{
  "name": "@test/garden-css-_template_",
  "version": "0.0.0",
  "repository": "zendeskgarden/css-_template_",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://zdrepo.jfrog.io/zdrepo/api/npm/npm"
  },
  "main": "dist/index.css",
  "style": "src/index.css",
  "devDependencies": {
    "cssnano": "^3.10.0",
    "gh-pages": "^1.0.0",
    "live-server": "^1.2.0",
    "postcss-cli": "^4.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "pre-commit": "^1.2.2",
    "stylelint": "^7.10.1",
    "stylelint-config-zendesk": "git+ssh://git@github.com/zendeskgarden/stylelint-config.git#4.1.0",
    "stylelint-order": "^0.3.0",
    "zd-css-variables": "git+ssh://git@github.com/zendeskgarden/css-variables.git#1.1.0"
  },
  "scripts": {
    "build": "postcss src/index.css --output dist/index.css",
    "lint": "stylelint $(find src -name '*.css')",
    "nuke": "rm -rf node_modules",
    "publish": "gh-pages -d demo",
    "rebuild": "npm run nuke && npm install && npm run build",
    "start": "npm run watch & live-server demo",
    "test": "npm run lint",
    "watch": "npm run build -- --watch"
  },
  "pre-commit": [
    "build",
    "test"
  ]
}
