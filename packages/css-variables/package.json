{
  "name": "zd-css-variables",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "style": "dist/index.css",
  "devDependencies": {
    "cssnano": "^3.10.0",
    "jsonlint": "^1.6.2",
    "postcss": "^5.2.17",
    "pre-commit": "^1.2.2",
    "stylelint": "^7.10.1",
    "stylelint-config-zendesk": "git+ssh://git@github.com/zendeskgarden/stylelint-config.git#4.1.0",
    "stylelint-order": "^0.3.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "_jsonlint": "jsonlint dist/index.json -q",
    "_stylelint": "stylelint dist/index.css",
    "build": "node build",
    "nuke": "rm -rf node_modules",
    "rebuild": "npm run nuke && npm install && npm run build",
    "test": "npm run _stylelint && npm run _jsonlint",
    "watch": "watch 'npm run build'"
  },
  "pre-commit": [
    "build",
    "test"
  ]
