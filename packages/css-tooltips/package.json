{
  "name": "zd-css-tooltips",
  "version": "1.1.0",
  "private": true,
  "main": "dist/index.css",
  "style": "src/index.css",
  "devDependencies": {
    "cssnano": "^3.10.0",
    "gh-pages": "^0.12.0",
    "live-server": "^1.2.0",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "pre-commit": "^1.2.2",
    "stylelint": "^7.7.1",
    "stylelint-config-zendesk": "git+ssh://git@github.com/zendeskgarden/stylelint-config.git#4.0.0",
    "zd-css-variables": "git+ssh://git@github.com/zendeskgarden/css-variables.git#0.10.0"
  },
  "scripts": {
    "build": "postcss --config .postcss.json",
    "lint": "stylelint $(find src -name '*.css')",
    "nuke": "rm -rf node_modules",
    "publish": "gh-pages -d demo",
    "rebuild": "npm run nuke && npm install && npm run build",
    "start": "npm run watch & live-server demo",
    "test": "npm run lint",
    "watch": "npm run build -- --watch"
  },
  "pre-commit": [
    "build",
    "test"
  ]
}
