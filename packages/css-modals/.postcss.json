{
  "input": "src/index.css",
  "output": "dist/index.css",
  "use": [
    "postcss-import",
    "postcss-cssnext",
    "postcss-inline-svg",
    "cssnano"
  ],
  "cssnano": {
    "autoprefixer": false,
    "core": false,
    "discardComments": { "removeAll": true },
    "reduceIdents": false,
    "zindex": false
  },
  "postcss-cssnext": {
    "features": { "colorRgba": false }
  },
  "postcss-inline-svg": {
    "path": "node_modules/zd-svg-icons/src"
  }
}
