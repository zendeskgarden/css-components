{
  "plugins": {
    "postcss-import",
    "postcss-cssnext": {
      "features": { "colorRgba": false }
    },
    "postcss-inline-svg": {
      "path": "node_modules/@zendesk/garden-svg-icons/src"
    },
    "cssnano": {
      "autoprefixer": false,
      "core": false,
      "discardComments": { "removeAll": true },
      "reduceIdents": false,
      "zindex": false
    }
  }
}
