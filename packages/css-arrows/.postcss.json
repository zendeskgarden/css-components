{
  "input": "src/index.css",
  "output": "dist/index.css",
  "use": [
    "postcss-import",
    "postcss-cssnext",
    "cssnano"
  ],
  "cssnano": {
    "core": false,
    "discardComments": { "removeAll": true }
  },
  "postcss-cssnext": {
    "browsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "Explorer 9"
    ]
  }
}
