{
  "input": "src/index.css",
  "output": "dist/index.css",
  "use": [
    "postcss-import",
    "postcss-input-range",
    "postcss-cssnext",
    "postcss-inline-svg",
    "cssnano"
  ],
  "cssnano": {
    "autoprefixer": false,
    "core": false,
    "discardComments": { "removeAll": true },
    "zindex": false
  },
  "postcss-inline-svg": {
    "path": "node_modules/zd-svg-icons/src"
  }
}
