{
  "private": true,
  "name": "@zendeskgarden/css-components",
  "version": "0.0.0",
  "scripts": {
    "build": "lerna run build --stream",
    "build:demo": "storybook build -o demo",
    "format": "prettier-package-json --write && lerna run format --stream",
    "postinstall": "husky install",
    "lint": "eslint scripts/*.mjs && lerna run lint --stream",
    "prepare": "yarn build",
    "start": "storybook dev -p 6006",
    "tag": "lerna version --conventional-commits",
    "test": "lerna run test --stream"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "string-width": "4.2.3",
    "wrap-ansi": "7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/eslint-parser": "7.22.15",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@storybook/addon-essentials": "7.5.0",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/react": "7.5.0",
    "@storybook/react-webpack5": "7.5.0",
    "@zendeskgarden/css-grid": "0.1.39",
    "@zendeskgarden/css-utilities": "4.5.5",
    "@zendeskgarden/eslint-config": "34.0.0",
    "@zendeskgarden/react-theming": "8.73.0",
    "@zendeskgarden/scripts": "2.0.2",
    "@zendeskgarden/stylelint-config": "20.0.0",
    "@zendeskgarden/svg-icons": "7.0.0",
    "cssnano": "6.0.1",
    "envalid": "7.3.1",
    "eslint": "8.51.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "focus-visible": "5.2.0",
    "husky": "8.0.3",
    "jsonlint": "1.6.3",
    "lerna": "7.3.1",
    "polished": "4.2.2",
    "postcss": "8.4.31",
    "postcss-cli": "10.1.0",
    "postcss-import": "15.1.0",
    "postcss-inline-svg": "6.0.0",
    "postcss-input-range": "5.0.0",
    "postcss-preset-env": "9.2.0",
    "prettier-package-json": "2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "storybook": "7.5.0",
    "styled-components": "6.1.0",
    "stylelint": "15.11.0",
    "stylelint-order": "6.0.3",
    "stylelint-scss": "5.2.1",
    "watch": "1.0.2"
  }
